<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<define name="dailyBuild">
			<include name="smoke" />
			<include name="sanity" />
		</define>
		<run>
			<include name="dailyBuild" />
		</run>
	</groups>
	<parameter name="browserName" value="edge" />
	<test name="Sanity Test">
		<classes>
			<class name="com.smart.groups.TestClass" />
			<class name="com.smart.groups.BaseClass" />
		</classes>
	</test>
</suite>  
